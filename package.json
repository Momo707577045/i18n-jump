{
  "name": "i18n-jump",
  "version": "0.0.17",
  "publisher": "maojingwen",
  "displayName": "i18n-jump",
  "description": "i18n-jump-for-xmp",
  "icon": "readme/000.png",
  "keywords": [
    "i18n",
    "xmp"
  ],
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Momo707577045/i18n-jump"
  },
  "main": "./out/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "i18n-jump.search-i18n",
        "key": "shift+alt+1",
        "mac": "shift+alt+1",
        "when": "editorHasSelection"
      },
      {
        "command": "i18n-jump.jump-i18n-cn",
        "key": "shift+alt+2",
        "mac": "shift+alt+2",
        "when": "editorHasSelection"
      },
      {
        "command": "i18n-jump.jump-store",
        "key": "shift+alt+3",
        "mac": "shift+alt+3",
        "when": "editorHasSelection"
      }
    ],
    "commands": [
      {
        "command": "i18n-jump.search-i18n",
        "title": "搜索被引用项"
      },
      {
        "command": "i18n-jump.jump-i18n-cn",
        "title": "跳转至中文翻译"
      },
      {
        "command": "i18n-jump.jump-i18n-en",
        "title": "跳转至英文翻译"
      },
      {
        "command": "i18n-jump.jump-store",
        "title": "跳转至 store 定义"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "i18n-jump.search-i18n",
          "group": "navigation"
        },
        {
          "command": "i18n-jump.jump-i18n-cn",
          "group": "navigation"
        },
        {
          "command": "i18n-jump.jump-i18n-en",
          "group": "navigation"
        },
        {
          "command": "i18n-jump.jump-store",
          "group": "navigation"
        }
      ]
    }
  },
  "activationEvents": [
    "*"
  ],
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.76.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.2.3"
  },
  "dependencies": {
    "@types/lodash": "^4.14.194",
    "lodash": "^4.17.21"
  },
  "npm": {
    "packageManager": "auto"
  }
}